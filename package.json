{
  "name": "x-cli",
  "version": "0.1.0",
  "description": "A fast, type-safe CLI for X (Twitter)",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "x": "./dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "bunx 0xc .",
    "format": "bunx 0xc --fix ."
  },
  "dependencies": {
    "arctic": "^3.5.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^13.1.0",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.16",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "twitter",
    "x",
    "cli",
    "api"
  ]
}
